@model Ts3.pl.Models.Users

@{
    ViewBag.Title = "Registry";
    Layout = "~/Views/_SharedView.cshtml";

}
<style>
    label {
        color: #428bca !important;
        text-decoration: none;
    }
</style>


<body>

    @if (!string.IsNullOrEmpty(Html.ViewBag.ErrorMsg))
    {
        <div class="alert alert-danger">
            <a href="#" class="close" data-dismiss="alert">&times;</a>
            <strong>Błąd!</strong> @ViewBag.ErrorMsg
        </div>
    }
    <div class="panel panel-info">
        <div class="panel-heading">
            <h3 class="panel-title">Rejestracja - utwórz konto! / Logowanie - zaloguj się </h3>
        </div>
        <div class="panel-body">

            <div class="row">


                @Html.Partial("_Registry", Model)



            </div>
        </div>
    </div>

</body>

@Scripts.Render("~/bundles/bootstrap")
@Styles.Render("~/Content/bootstrap")




